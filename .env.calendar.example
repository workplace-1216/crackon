# Google Calendar OAuth Configuration
# Get these from Google Cloud Console -> APIs & Services -> Credentials
GOOGLE_CLIENT_ID=example
GOOGLE_CLIENT_SECRET=example-secret
GOOGLE_REDIRECT_URI=https://yourdomain.com/api/calendars/callback

# Microsoft Graph OAuth Configuration  
# Get these from Azure Portal -> App registrations
MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here
MICROSOFT_REDIRECT_URI=https://yourdomain.com/api/calendars/callback

# For development, you can use localhost:
# GOOGLE_REDIRECT_URI=http://localhost:3000/api/calendars/callback
# MICROSOFT_REDIRECT_URI=http://localhost:3000/api/calendars/callback